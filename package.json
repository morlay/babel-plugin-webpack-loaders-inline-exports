    "test": "npm run lint && npm run mocha && npm run build && npm run examples",
    "webpack2": "npm i webpack@2.1.0-beta.20 && npm test",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "WTFPL",
  "devDependencies": {
    "@morlay/babel-preset": "1.0.x",
    "@morlay/eslint-config": "1.0.x",
