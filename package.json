{
  "name": "babel-plugin-webpack-loaders-inline-exports",
  "version": "0.2.1",
  "main": "lib/index.js",
  "config": {
    "MOCHA_OPTS": "-R dot --compilers js:babel-core/register src/__tests__/*.spec.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "mocha": "mocha $npm_package_config_MOCHA_OPTS",
